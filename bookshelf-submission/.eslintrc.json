{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "arrow-body-style": "off",
    "prefer-promise-reject-errors": "off",
    "no-shadow": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "comma-dangle": "off",
    "eqeqeq": "off"
  }
}
